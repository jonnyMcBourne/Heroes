 ({ history, location }) => {\n \n  //state\n  const [{ text }, handleInputChange, reset] = useForm({ text: \"\" });\n  const [value, setValue] = useState(\"\");\n  const [isDisabled, setIsDisabled] = useState(true);\n\n  const locationhook = useLocation();\n  const trimmed = locationhook.search.substring(1);\n  const parsed = queryString.parse(trimmed);\n  const { q } = parsed;\n  \n\n  useEffect(() => {\n    if (text === \"\") {\n      setValue(q);\n    }\n  }, []);\n\n  useEffect(() => {\n    if (text === \"\") {\n      setIsDisabled(true);\n      setValue(text);\n      reset();\n    } else {\n      setIsDisabled(false);\n      setValue(text);\n    }\n  }, [text]);\n\n  //data\n  const heroesFiltered = getheroesByName(q);\n\n  const findHero = (e) => {\n    e.preventDefault();\n    history.push(`?q=${text}`);\n    reset();\n  };\n\n  return (\n    <>\n      <div className=\"row\">\n        <div className=\"col-5\">\n          <form onSubmit={findHero}>\n            <input\n              className=\"form-control\"\n              type=\"text\"\n              placeholder=\"Find your Hero\"\n              name=\"text\"\n              value={value}\n              onChange={handleInputChange}\n              autoComplete=\"off\"\n            />\n            <button\n              className=\"btn btn-outline-primary btn-block mt-3\"\n              disabled={isDisabled}\n            >\n              Find\n            </button>\n          </form>\n        </div>\n        <div className=\"col-7\">\n          {heroesFiltered.map((hero) => (\n            <HeroCard key={hero.id} {...hero} />\n          ))}\n        </div>\n      </div>\n    </>\n  );\n};\n","/home/jonathan/Desktop/CODE/reactjs/Curso/heroes/src/hooks/useForm.js",[],"/home/jonathan/Desktop/CODE/reactjs/Curso/heroes/src/App.js",[],"/home/jonathan/Desktop/CODE/reactjs/Curso/heroes/src/auth/authContext.js",[],"/home/jonathan/Desktop/CODE/reactjs/Curso/heroes/src/types/types.js",[],"/home/jonathan/Desktop/CODE/react